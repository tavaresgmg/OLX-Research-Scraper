# Configurações de Banco de Dados
DB_USER=olxscraper
DB_PASSWORD=securepassword
DB_NAME=olxdb
DATABASE_NAME=data/olx_precos.db

# Configurações do Redis
REDIS_ENABLED=true
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=
REDIS_TTL=86400

# Configurações da Aplicação
LOG_LEVEL=INFO
CACHE_TTL=3600

# Parâmetros para o docker-compose
PRODUTOS=iphone,macbook,ipad
OPCOES=--min-preco=500 --max-preco=5000

# Configurações de Proxy (lista separada por vírgulas)
PROXY_LIST=http://user:pass@proxy1.example.com:8080,http://user:pass@proxy2.example.com:8080
PROXY_TEST_URL=https://www.olx.com.br
PROXY_MIN_SUCCESS_RATE=0.5
PROXY_COOLDOWN_TIME=300

# Configurações do scraper
DEFAULT_PAGES=3
DEFAULT_STATE=estado-go
MIN_PRICE=50
MAX_PRICE=100000
